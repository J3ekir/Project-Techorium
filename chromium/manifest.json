{
    "manifest_version": 3,
    "name": "Project Techorium",
    "description": "Stereo.Luigi99 tarafından yapıldı. Yine... Eklentide kullanılan Techolay logosu dijitalfikir'e aittir. Ne yalan söyleyeyim, logo cidden çok güzel. Yapana sonsuz teşekkürler. Tek olay, Techolay!",
    "homepage_url": "https://techolay.net/sosyal/uye/stereo-luigi99.191/",
    "version": "1.2",
    "icons": {
      "16": "techorium-icons/icon16.png",
      "32": "techorium-icons/icon32.png",
      "64": "techorium-icons/icon64.png",
      "128": "techorium-icons/icon128.png"
    },
    "permissions": [
      "declarativeNetRequest"
    ],
    "host_permissions": [
      "https://techolay.net/*"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    },
    "web_accessible_resources": [
      {
        "resources": [ "/*" ],
        "matches": [ "https://techolay.net/*" ]
      }
    ],
    "content_scripts": [
      {
        "js": [
          "/js/scriptOS.js", "/js/otherThings.js"
        ],
        "matches": [
          "https://techolay.net/*"
        ],
        "css": [
          "/css/hideOS-fa.css", "/css/osBrowser.css",  "/css/otherThings.css"
        ],
        "run_at": "document_start"
      }
    ]
  }